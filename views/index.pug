extends layout

block content
  h1.titulo Administrador de Bancos
  .campos
    input#campo1(type="text", name="campo1" placeholder="Nombre")
    input#campo2(type="text", name="campo2" placeholder="Cadena Mundial")
    input#campo3(type="text", name="campo3" placeholder="Años")
    input.btn(type="button", value="Guardar", href="") 
  table 
    thead
      tr
        th id
        th Nombre
        th Cadena Mundial
        th Años
        th Opciones
    tbody.datos

